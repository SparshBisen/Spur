{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.ts"
    },
    {
      "src": "/api/chat/message",
      "dest": "/api/chat/message.ts"
    },
    {
      "src": "/api/chat/history",
      "dest": "/api/chat/history.ts"
    }
  ]
}